generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  role  String
}

model Project {
  id        Int     @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
}
